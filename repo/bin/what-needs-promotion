#!/bin/bash

BINDIR=$(dirname $0)
DISTS=$BINDIR/../dists
DIST=${1:-sid}
[ -t 1 ] && TO_TERM="-t"

$BINDIR/diff-dists $TO_TERM \
	<(bzcat $DISTS/$DIST-pgdg-testing/main/source/Sources.bz2) \
	<(bzcat $DISTS/$DIST-pgdg/main/source/Sources.bz2) |
grep -v -e '^bordeaux-threads' \
	-e '^buildapp' \
	-e '^cffi' \
	-e '^cl-' \
	-e '^pgdg-buildenv'
